@model List<QualificationModel>

@for (int i = 0; i < Model.Count; i++)
{
    <div class="form-group">
        <label asp-for="@Model[i].Degree"></label>
        <input asp-for="@Model[i].Degree" class="form-control" />
        <span asp-validation-for="@Model[i].Degree" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model[i].Grade"></label>
        <input asp-for="@Model[i].Grade" class="form-control" />
        <span asp-validation-for="@Model[i].Grade" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model[i].Institution"></label>
        <input asp-for="@Model[i].Institution" class="form-control" />
        <span asp-validation-for="@Model[i].Institution" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model[i].GraduationDate"></label>
        <input asp-for="@Model[i].GraduationDate" class="form-control" type="date" />
        <span asp-validation-for="@Model[i].GraduationDate" class="text-danger"></span>
    </div>
    
}